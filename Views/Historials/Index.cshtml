@model IEnumerable<Ikigenda.Models.Historial>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Observaciones)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateLocal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Planin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoServicio)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observaciones)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateLocal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cliente.Documento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Planin.PlaninId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoServicio.TipoServicioId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.HistorialId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.HistorialId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.HistorialId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
